# ==== Banco ====
DB_SERVER=localhost\SQLEXPRESS
DB_NAME=MinhaBase
DB_USER=seu_usuario
DB_PASS=sua_senha
ODBC_DRIVER=ODBC Driver 18 for SQL Server

# Loop/polling
POLL_SECONDS=5
DEBOUNCE_SECONDS=0
TERCEIRO=DIAGNÓSTICO DO BRASIL - DB
FAILED_DIR=completo/failed_events

# ==== Bemsoft ====
BEMSOFT_BASE_URL=https://bemsoft.ws.wiselab.com.br
BEMSOFT_ENDPOINT=/requests
BEMSOFT_TOKEN=coloque_seu_token_aqui
BEMSOFT_TIMEOUT=30
BEMSOFT_RETRIES=3
BEMSOFT_BACKOFF=0.5
BEMSOFT_VERIFY=1
BEMSOFT_DRY_RUN=1

# Defaults se o legado não trouxer:
DEFAULT_GENDER=M
DEFAULT_BIRTHDATE=1970-01-01

# Médico (opcional)
PHYSICIAN_NAME=
PHYSICIAN_COUNCIL=
PHYSICIAN_NUMBER=
PHYSICIAN_UF=

# Mapeamento de exames (opcional)
# BEMSOFT_TEST_MAP_PATH=./tests_map.json

